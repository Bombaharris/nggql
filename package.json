{
  "name": "nggql",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "gql-codegen": "graphql-codegen --config graphql-codegen.yml",
    "gql-codegen:watch": "graphql-codegen --config graphql-codegen.yml --watch",
    "prestart": "npm run gql-codegen",
    "prettier:check": "prettier \"./src/**/*.ts\" --check",
    "prettier:format": "prettier \"./src/**/*.ts\" --write",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.6",
    "@angular/cdk": "^17.0.3",
    "@angular/common": "^17.0.6",
    "@angular/compiler": "^17.0.6",
    "@angular/core": "^17.0.6",
    "@angular/forms": "^17.0.6",
    "@angular/language-service": "^17.0.6",
    "@angular/platform-browser": "^17.0.6",
    "@angular/platform-browser-dynamic": "^17.0.6",
    "@angular/router": "^17.0.6",
    "@apollo/client": "^3.8.8",
    "apollo-angular": "^5.0.1",
    "graphql": "^16.6.0",
    "ng-zorro-antd": "^17.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.6",
    "@angular-eslint/builder": "^17.1.1",
    "@angular-eslint/eslint-plugin": "^17.1.1",
    "@angular-eslint/eslint-plugin-template": "^17.1.1",
    "@angular-eslint/schematics": "^17.1.1",
    "@angular-eslint/template-parser": "^17.1.1",
    "@angular/cli": "^17.0.6",
    "@angular/compiler-cli": "^17.0.6",
    "@graphql-codegen/cli": "^2.13.2",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript-apollo-angular": "^3.5.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@stylistic/eslint-plugin-js": "^1.5.1",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2"
  }
}
